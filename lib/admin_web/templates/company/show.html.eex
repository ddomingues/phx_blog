<h1>Show Company</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @company.name %>
  </li>

  <li>
    <strong>Fantasy name:</strong>
    <%= @company.fantasy_name %>
  </li>

  <li>
    <strong>Document number:</strong>
    <%= @company.document_number %>
